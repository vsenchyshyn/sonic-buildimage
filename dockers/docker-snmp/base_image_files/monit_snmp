###############################################################################
## Monit configuration for snmp container
## process list:
##  snmpd
##  snmpd_subagent
###############################################################################
check process snmpd matching "/usr/sbin/snmpd\s"
    if does not exist for 5 times within 5 cycles then alert

check process snmp_subagent matching "python3 -m sonic_ax_impl"
    if does not exist for 5 times within 5 cycles then alert
